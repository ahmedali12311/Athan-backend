{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "%DOMAIN%/city.json",
    "description": "city model schema",
    "type": "object",
    "additionalProperties": true,
    "required": [
        "name"
    ],
    "properties": {
        "id": {
            "type": "string",
            "format": "uuid"
        },
        "name": {
            "type": "string",
            "minLength": 1,
            "maxLength": 50
        },
        "is_disabled": {
            "type": "boolean"
        },
        "location": {
            "$ref": "%DOMAIN%/properties/point_null.json"
        },
        "created_at": {
            "type": "string",
            "format": "date-time"
        },
        "updated_at": {
            "type": "string",
            "format": "date-time"
        }
    }
}